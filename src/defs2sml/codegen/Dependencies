smltype: /ast/type /util/util /ast/name
tinyc: /util/util
typeinfo: /util/util /ast/ast /ast/name /ast/type smltype tinyc 
smlgen: /ast/ast /ast/name /ast/type /util/ smltype typeinfo
cgen: /ast/ast /ast/name /ast/type tinyc smltype typeinfo